{
  "expo": {
    "name": "LNU-ILMS",
    "slug": "LNU-ILMS",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/lnuicon.png",
    "scheme": "lnu-ilms",
    "userInterfaceStyle": "automatic",
    "splash": {
      "image": "./assets/images/lnuicon.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/lnuicon.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.capstone.lnu_ilms"
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router"
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "0bbe347c-cb05-453d-a7c2-c6858cecd971"
      }
    }
  }
}
